    <queryString language="SQL">
        <![CDATA[SELECT
    CASE
        WHEN r.activo = 1 THEN 'Activo'
        ELSE 'Anulado'
    END AS estado_activo,
    CASE
        WHEN r.estadoreserva = 1 THEN 'Pagado'
        ELSE 'Pendiente'
    END AS estado_reserva,
    r.fechaentrada,
    r.fechasalida,
    c.nombre AS nombre_cliente,
    h.numeroHabitacion AS numero_habitacion
FROM
    reserva r
INNER JOIN
    cliente c ON r.IDCliente = c.IDCliente
INNER JOIN
    habitacion h ON r.IDHabitacion = h.IDHabitacion
WHERE
    r.fechaCreacion >= DATE_SUB(NOW(), INTERVAL 7 DAY);]]>
    </queryString>
