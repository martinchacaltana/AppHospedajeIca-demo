    <queryString language="SQL">
        <![CDATA[SELECT
    th.IdTipohabitacion,
    th.descripcion,
    COUNT(r.IDhabitacion) AS cantidad_reservas
FROM
    reserva r
INNER JOIN
    tipohabitacion th ON r.IDhabitacion = th.IdTipohabitacion
WHERE
    r.activo = 1 AND th.activo = 1
GROUP BY
    th.IdTipohabitacion, th.descripcion
ORDER BY
    cantidad_reservas DESC
LIMIT 2;]]>
    </queryString>
